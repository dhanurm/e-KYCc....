  <form method="post">
  <h1>Sign Up</h1>
  <div class="form-group">
  <label="name">First Name:</label>
  <input type="text" class="form-control" name="firstname">
  </div>
  <div class="form-group">
  <label="name">Last Name:</label>
  <input type="text" class="form-control" name="lastname">
  </div>
  <div class="form-group">
  <label="name">Mobile:</label>
  <input type="number" class="form-control" name="mobile">
  </div>
  <div class="form-group">
  <label="name">Date of Birth:</label>
  <input type="date" class="form-control" name="dob">
  </div>
  <div class="form-group">
  <label="name">Aadhar Card:</label>
  <input type="number" class="form-control" name="aadhar">
  </div>
  <div class="form-group">
  <label="name">PAN Card:</label>
  <input type="text" class="form-control" name="pan">
  </div>
  <div class="form-group">
  <label="name">Email:</label>
  <input type="text" class="form-control" name="email">
  </div>
  <div class="form-group">
  <label="name">Password:</label>
  <input type="password" class="form-control" name="password">
  </div>
  <button type="submit" class="btn btn-success">Submit</button>
  </form>
