    <style type="text/css">
    	
#button{
  		 padding: 10px 22px;
		width:40%;
		position: relative;
	
		margin-left:100px;
  	}

  .form-control
  	{
  		padding: 10px 22px;
		width:70%;
		position: relative;
	    left:60px;	
  			right:40px;
  	}
    .mainhead{

      margin-top: 570px;
      margin-left: 230px;

    }
