<body>  
    <div class="site-wrapper">
    <% if(errors.length >0){ %>
    <div class="alert alert-danger alert-dismissible" role="alert">
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
      <%= errors %>
    </div>

    <%}%>

      <div class="site-wrapper-inner">

        <div class="cover-container">
        <div class="masthead clearfix">
        <div class="mainhead">
            <div class="inner">
              <h6 class="masthead-brand" style="margin: center">Powered by <a href="www.colonus.io" style="color: green;">Colonus</a>
              </h6>
            </div>
          </div>
</div>
          
          <div class="inner cover">
          	<div class="container">
    
      <form  name ="form1" class="form-signin" method="post" action ="#">
        <h2 class="form-signin-heading"> Sign Up</h2></br></br></br>
        <input type="text" id="aadhar" maxlength="12" class="form-control" name="aadhar" placeholder="Aadhar Card number" title="Enter valid  Aadhar Number " required autofocus>
        <input type="text" class="form-control"  name ="pan" placeholder="PAN Card number" title="Enter valid PAN Number" style ="text-transform:uppercase;" required autofocus>
        <input type="text" name="dob" id="datepicker" class="form-control" size="30" placeholder="Date of Birth"  required autofocus>
       </br>
        <button type="submit"  id="button" class="btn btn-lg btn-primary btn-block btn-success" type="submit" style="color: black;"><aSign Up</button>
        

      </form>
          </div>
          </div>
        </div>
      </div>
    </div>

 
  <script type="text/javascript">

   $('document').ready(function(){

      $("#datepicker").datepicker({
       dateFormat:"dd-mm-yy",
      showWeek: true,
      changeMonth: true,
      changeYear: true,
      yearRange:"-116:+0",
        beforeShow: function (textbox, instance) {   
        instance.dpDiv.css({
            marginLeft: textbox.offsetWidth+(7)+'px'
        });
        }

      //maxDate:new Date(currentYear,CurrentMonth,CurrentDate)
    });
  });
 
</script>
